{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"subcommand": "test",
			"problemMatcher": [
				"$rustc"
			],
			"group": "test",
			"label": "Run all tests"
		},
		{
			"command": "cargo",
			"args": [
				"test",
				"--test",
				"integration_tests"
			],
			"problemMatcher": [
				"$rustc"
			],
			"group": "test",
			"label": "Run integration tests"
		}
	]
}